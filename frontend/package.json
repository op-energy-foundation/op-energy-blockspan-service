{
  "name": "ope-blockspan-frontend",
  "version": "1.0.0",
  "description": "Blockrate Binaries",
  "license": "GNU Affero General Public License v3.0",
  "homepage": "http://exchange.op-energy.info",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/girish-velas/ope-blockspan-service"
  },
  "bugs": {
    "url": "https://github.com/girish-velas/ope-blockspan-service/issues"
  },
  "keywords": [
    "Blockrate Binaries",
    "ope",
    "Binaries",
    "Blockrate"
  ],
  "main": "index.ts",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng.js",
    "tsc": "./node_modules/typescript/bin/tsc",
    "serve": "npm run ng -- serve -c local",
    "serve:stg": "npm run ng -- serve -c staging",
    "serve:local-prod": "npm run ng -- serve -c local-prod",
    "serve:local-staging": "npm run ng -- serve -c local-staging",
    "start": "npm run ng -- serve -c local",
    "start:stg": "npm run ng -- serve -c staging",
    "start:local-prod": "npm run ng -- serve -c local-prod",
    "start:local-staging": "npm run ng -- serve -c local-staging",
    "build": "npm run ng -- build --configuration production",
    "test": "npm run ng -- test",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "lint:fix": "./node_modules/.bin/eslint . --ext .ts --fix",
    "prettier": "prettier --write \"src/app/**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "e2e": "npm run ng -- e2e",
    "e2e:ci": "npm run cypress:run:ci",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "cypress:open:ci": "start-server-and-test serve:local-prod 4200 cypress:open",
    "cypress:run:ci": "start-server-and-test serve:local-prod 4200 cypress:run:record",
    "cypress:open:ci:staging": "start-server-and-test serve:local-staging 4200 cypress:open",
    "cypress:run:ci:staging": "start-server-and-test serve:local-staging 4200 cypress:run:record"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular/animations": "~13.3.10",
    "@angular/cli": "~13.3.7",
    "@angular/common": "~13.3.10",
    "@angular/compiler": "~13.3.10",
    "@angular/core": "~13.3.10",
    "@angular/forms": "~13.3.10",
    "@angular/localize": "~13.3.10",
    "@angular/platform-browser": "~13.3.10",
    "@angular/platform-browser-dynamic": "~13.3.10",
    "@angular/platform-server": "~13.3.10",
    "@angular/router": "~13.3.10",
    "@fortawesome/angular-fontawesome": "~0.10.2",
    "@fortawesome/fontawesome-common-types": "~6.1.1",
    "@fortawesome/fontawesome-svg-core": "~6.1.1",
    "@fortawesome/free-solid-svg-icons": "~6.1.1",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@types/qrcode": "~1.4.2",
    "angular-svg-icon": "^13.0.0",
    "bootstrap": "~4.5.0",
    "browserify": "^17.0.0",
    "@types/swagger-ui-dist": "^3.30.1",
    "swagger-ui-dist": "~4.12.0",
    "path": "^0.12.7",
    "ngx-toastr": "^14.3.0",
    "rxjs": "~7.5.5",
    "tinyify": "^3.0.0",
    "tlite": "^0.1.9",
    "tslib": "~2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~13.3.10",
    "@angular/language-service": "~13.3.10",
    "@nguniversal/builders": "~13.1.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "http-proxy-middleware": "~2.0.6",
    "prettier": "^2.7.1",
    "ts-node": "~10.8.1",
    "typescript": "~4.6.4"
  },
  "optionalDependencies": {
    "@cypress/schematic": "~2.0.0",
    "cypress": "^10.3.0",
    "cypress-fail-on-console-error": "~3.0.0",
    "cypress-wait-until": "^1.7.2",
    "mock-socket": "~9.1.4",
    "start-server-and-test": "~1.14.0"
  },
  "scarfSettings": {
    "enabled": false
  }
}
